{
  "name": "svelte-time-series",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.mjs",
  "svelte": "./src/index.svelte",
  "description": "display time series data with svelte",
  "keywords": [
    "component",
    "svelte",
    "time-series",
    "vite",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "vite build",
    "start": "vite",
    "test": "npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --page-request-timeout 9000 --app-init-delay 3000 --app vite",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "documentation lint ./src/index.mjs && npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs",
    "preview": "vite preview"
  },
  "dependencies": {
    "d3-scale": "^4.0.0",
    "mf-hosting": "^1.7.9"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "documentation": "^14.0.0",
    "npm-pkgbuild": "^10.11.3",
    "semantic-release": "^19.0.3",
    "svelte": "^3.49.0",
    "testcafe": "^2.0.0",
    "vite": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-time-series.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-time-series/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-time-series#readme",
  "pkgbuild": {
    "content": {},
    "groups": "examples"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --available --continue --publish dist"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist/*.deb",
              "label": "Debian Package"
            },
            {
              "path": "dist/*.pkg.*",
              "label": "Arch Linux Package"
            }
          ]
        }
      ]
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-cloudflare",
      "arlac77/template-pacman",
      "arlac77/template-svelte-component"
    ]
  }
}
